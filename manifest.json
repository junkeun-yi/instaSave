{
  "name": "InstaSave",
  "description": "Download pictures from Instagram!",
  "version": "1.0",
  "icons": { "16": "instaSave_logo.png" },
  "manifest_version": 2,
  "browser_action": {
        "default_icon": "instaSave_logo.png"
    },
  "content_scripts": [
    {
      "matches": [
        "https://*.instagram.com/*","http://*.instagram.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
