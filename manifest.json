{
  "name": "InstaSave",
  "description": "Download pictures from Instagram!",
  "version": "1.0",
  "icons": { "16": "instaSave_logo.png" },
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
  "content_scripts": [
  {
    "matches": [
      "https://*.instagram.com/*","http://*.instagram.com/*"
    ],
    "js": ["content.js"]
  }
]
}
